n = 1000000;

res = [];
for k = 1: 100
    y = [];
    y(1) = 0.0;
    for i =2:n
        flip = randi(2);
        if(flip==1)
           y(i) = y(i-1) + 1.0;
        else
            y(i) = y(i-1);
        end
    end
    res(k) = y;
    line(1:n/10000:n, y, rand(1,3)); 
end
figure(1)
plot(1:1:n, y, 'r');