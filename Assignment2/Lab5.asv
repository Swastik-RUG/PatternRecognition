x1 = -10:1:20;
F = -8.81887145067;
y1 = [];
y2 = [];
for i=1:length(x1)
    x = x1(i);
    a = 2*(x*x)-16*x+8*F;
    tmp = sqrt(4 - 12*a)/(2*a);
    y1(i) = 2+tmp;
    y2(i) = 2-tmp;
end

figure(1)
a = plot(x1, real(y1), 'b-o');
hold on;
b = plot(x1, real(y2), 'r.-');
legend([a, b], {'x2 = 2+sqrt()', 'Set D'});
